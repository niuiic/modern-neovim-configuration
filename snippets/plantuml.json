{
  "panic": {
    "prefix": "panic",
    "body": [":error;", "kill"]
  },
  "flow": {
    "prefix": "flow",
    "body": [
      "@startuml",
      "title ${1}",
      "",
      "start",
      "",
      "${2}",
      "",
      "end",
      "@enduml"
    ]
  },
  "eventStorm": {
    "prefix": "eventStorm",
    "body": [
      "@startuml",
      "allowmixing",
      "skinparam rectangle {",
      "    BackgroundColor<<Event>> #FFD700",
      "    BackgroundColor<<Command>> #87CEEB",
      "    BackgroundColor<<Role>> #9370DB",
      "    BackgroundColor<<BusinessRule>> #FF6347",
      "    BackgroundColor<<Entity>> #20B2AA",
      "    BackgroundColor<<ValueObject>> #F0E68C",
      "    BackgroundColor<<Aggregate>> #98FB98",
      "    BackgroundColor<<AggregateRoot>> #32CD32",
      "    BackgroundColor<<BoundedContext>> #DDA0DD",
      "    BackgroundColor<<Policy>> #FFB6C1",
      "    BackgroundColor<<ReadModel>> #D2B48C",
      "    BackgroundColor<<Repository>> #8FBC8F",
      "    BackgroundColor<<Service>> #FF8C00",
      "}",
      "${1}",
      "@enduml"
    ]
  },
  "taskStorm": {
    "prefix": "taskStorm",
    "body": [
      "@startuml",
      "allowmixing",
      "skinparam rectangle {",
      "    BackgroundColor<<Task>> #FFB347",
      "    BackgroundColor<<Step>> #EEDDFF",
      "    BackgroundColor<<Issue>> #FF4444",
      "    BackgroundColor<<RootCause>> #8B0000",
      "    BackgroundColor<<Solution>> #66BB6A",
      "    BackgroundColor<<Checkpoint>> #5F9EA0",
      "    BackgroundColor<<Dependency>> #7B68EE",
      "    BackgroundColor<<Milestone>> #FFC300",
      "    BackgroundColor<<Impact>> #FF85C0",
      "    BackgroundColor<<Improvement>> #48D1CC",
      "}",
      "${1}",
      "@enduml"
    ]
  },
  "task": {
    "prefix": "TASK",
    "body": "rectangle ${1} <<Task>>"
  },
  "step": {
    "prefix": "STEP",
    "body": "rectangle ${1} <<Step>>"
  },
  "issue": {
    "prefix": "ISSUE",
    "body": "rectangle ${1} <<Issue>>"
  },
  "rootCause": {
    "prefix": "ROOTCAUSE",
    "body": "rectangle ${1} <<RootCause>>"
  },
  "solution": {
    "prefix": "SOLUTION",
    "body": "rectangle ${1} <<Solution>>"
  },
  "checkpoint": {
    "prefix": "CHECKPOINT",
    "body": "rectangle ${1} <<Checkpoint>>"
  },
  "dependency": {
    "prefix": "DEPENDENCY",
    "body": "rectangle ${1} <<Dependency>>"
  },
  "milestone": {
    "prefix": "MILESTONE",
    "body": "rectangle ${1} <<Milestone>>"
  },
  "impact": {
    "prefix": "IMPACT",
    "body": "rectangle ${1} <<Impact>>"
  },
  "improvement": {
    "prefix": "IMPROVEMENT",
    "body": "rectangle ${1} <<Improvement>>"
  },
  "event": {
    "prefix": "EVENT",
    "body": "rectangle ${1} <<Event>>"
  }
}
