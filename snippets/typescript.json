{
  "echo": {
    "prefix": "echo",
    "body": ["console.log(${1})"]
  },
  "ct": {
    "prefix": "ct",
    "body": ["/**", " * ${1}", "*/"]
  },
  "ctil": {
    "prefix": "ctil",
    "body": ["/** ${1} */"]
  },
  "caught": {
    "prefix": "caught",
    "body": [".catch(() => {})"]
  },
  "impl": {
    "prefix": "impl",
    "body": ["throw new Error('not implemented')"]
  },
  "bail": {
    "prefix": "bail",
    "body": ["throw new Error(${1})"]
  },
  "pw": {
    "prefix": "pw",
    "body": [
      "const { promise, resolve, reject } = Promise.withResolvers<${1}>()"
    ]
  }
}
