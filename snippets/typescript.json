{
  "echo": {
    "prefix": "echo",
    "body": ["console.log(${1})"]
  },
  "ct": {
    "prefix": "ct",
    "body": ["/**", " * ${1}", "*/"]
  },
  "ctil": {
    "prefix": "ctil",
    "body": ["/** ${1} */"]
  },
  "ignore-error": {
    "prefix": "ignore-error",
    "body": [".catch(() => {})"]
  }
}
